{
  "presets": ["es2015", "react", "stage-1"],
  "env": {
    "node": {
      "plugins": [
        [
          "babel-plugin-transform-require-ignore",
          {
            "extensions": [".scss", ".css"]
          }
        ]
      ]
    }
  },
  "plugins": [
    "transform-class-properties",
    "transform-object-rest-spread",
    "transform-runtime",
    "react-hot-loader/babel",
    "transform-decorators-legacy"

  ],

}
