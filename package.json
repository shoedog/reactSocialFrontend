  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "__DEV__": true
    },
    "env": {
      "browser": true
    },
    "rules": {
      "arrow-parens": "off",
      "generator-star-spacing": "off",
      "import/no-extraneous-dependencies": "off",
      "react/forbid-prop-types": "off",
      "react/jsx-filename-extension": "off",
      "react/no-danger": "off",
      "react/no-unused-prop-types": "off",
      "react/require-extension": "off"
    }
  },
  "scripts": {
    "esdoc": "esdoc -c ./tools/esdoc.json && open ./docs/esdoc/index.html",
    "eslint": "eslint -c .eslintrc src",
    "dev": "concurrently --kill-others \"npm run dev-api\" \"npm run dev-server\" \"npm run webpack-dev-server\"",
    "dev-api": "cross-env NODE_ENV=development nodemon --exec babel-node ./bin/apiServer.js",
    "dev-server": "cross-env NODE_ENV=development nodemon --exec babel-node ./bin/renderingServer.js",
    "webpack-dev-server": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack/devConfig.js",
    "build": "cross-env NODE_ENV=production webpack --progress --color -p --config ./webpack/devConfig.js",
    "test": "mocha --require ./tools/testSetup.js --compilers js:babel-register \"src/**/test.js\"",
    "win-build": "set NODE_ENV=production && webpack --progress --color -p --config ./webpack/devConfig.js",
    "win-start": "start cmd.exe /c \"set NODE_ENV=development BABEL_ENV=dev NODE_PATH=$NODE_PATH:./src/shared && node ./bin/renderingServer/\"",
    "win-dev": "npm run win-start && webpack-dev-server --progress --color"
  }
}
