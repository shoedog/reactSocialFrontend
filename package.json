  "scripts": {
    "build:prod": "cross-env NODE_ENV='production' npm run build:client && npm run build:server",
    "start": "cross-env NODE_ENV='production' node ./public/static/dist/server.js --name moonwalk",
    "dev:serve": "cross-env NODE_ENV=development & concurrently \"npm run build:devServer\" \"npm run watch:dev\" \"npm run start:dev\" \"npm run dev:api\"",
    "watch:dev": "cross-env NODE_ENV=development webpack-dev-server -d --devtool eval --config webpack.config.js --content-base public/static/dist/ --inline --hot --port 8000",
    "build:devServer": "cross-env NODE_ENV=development webpack -d --config webpack.serverConfig.js",
    "dev:api": "cross-env NODE_ENV=development nodemon --exec babel-node ../cs419-backend/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon ./public/static/dist/server.js",
    "build:client": "cross-env NODE_ENV='production' webpack -p --config webpack.config.js --output-public-path /",
    "build:server": "cross-env NODE_ENV='production' webpack -p --config webpack.serverConfig.js",
    "esdoc": "esdoc -c ./tools/esdoc.json && open ./docs/esdoc/index.html",
    "eslint": "eslint -c .eslintrc src",
    "test": "npm run tape:test",
    "mocha:test": "mocha --require ./tools/testSetup.js --compilers js:babel-register src/**/**.test.js",
    "tape:test": "tape --require babel-register src/**/**.spec.js"
  }
